{
  "name": "project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest run",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "server:dev": "tsx watch server/src/index.ts",
    "server:start": "node server/dist/index.js",
    "server:build": "tsc -p server/tsconfig.json",
    "server:test": "vitest run --config server/vitest.config.ts",
    "server:lint": "eslint server/src/**/*.ts",
    "server:seed": "tsx server/src/seeds/seed.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run server:dev\""
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
